{
  "expo": {
    "name": "Blinqfix",
    "slug": "blinqfix",
    "scheme": "blinqfix",
    "version": "0.0.1",

    // Remove the top-level "jsEngine" so it doesn't break Web.
    // Instead, enable Hermes only for iOS/Android below.
    // "jsEngine": "hermes",

    "android": {
      "newArchEnabled": true,
      "jsEngine": "hermes"
    },
    "ios": {
      "newArchEnabled": true,
      "jsEngine": "hermes"
    },

    "extra": {
      "apiUrl": "http://192.168.0.191:8888"  // Just keep this single key
    },

    "platforms": ["ios", "android", "web"],
    "assetBundlePatterns": ["**/*"]
  }
}
